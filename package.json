{
  "name": "community-intelligence-platform",
  "version": "1.0.0",
  "description": "AI-powered platform for transforming community stories into actionable intelligence",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "node scripts/run-tests.js all",
    "test:unit": "node scripts/run-tests.js unit",
    "test:integration": "node scripts/run-tests.js integration",
    "test:e2e": "node scripts/run-tests.js e2e",
    "test:performance": "node scripts/run-tests.js performance",
    "test:watch": "node scripts/run-tests.js unit --watch",
    "test:coverage": "node scripts/run-tests.js unit --coverage",
    "test:ci": "node scripts/run-tests.js all --bail --coverage",
    "test:debug": "node scripts/run-tests.js unit --verbose",
    "db:setup": "npx supabase db reset",
    "db:migrate": "npx supabase db push",
    "db:seed": "npx supabase db seed",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "generate:types": "npx supabase gen types typescript --local > src/types/database.types.ts",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next dist coverage test-results",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.38.0",
    "chart.js": "^4.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.294.0",
    "mammoth": "^1.10.0",
    "next": "14.0.3",
    "openai": "^4.20.1",
    "pdf-lib": "^1.17.1",
    "pdf-poppler": "^0.2.3",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-leaflet": "^4.2.1",
    "react-player": "^2.13.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.2.2",
    "unpdf": "^1.1.0",
    "wavesurfer.js": "^7.4.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^14.2.33",
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.8",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-html-reporters": "^3.1.5",
    "jest-junit": "^16.0.0",
    "jest-util": "^29.7.0",
    "lint-staged": "^15.1.0",
    "playwright": "^1.40.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "supabase": "^1.123.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "projects": [
      "<rootDir>/jest.config.js"
    ]
  },
  "keywords": [
    "community intelligence",
    "indigenous communities",
    "ai analysis",
    "cultural safety",
    "story collection",
    "government transparency",
    "community empowerment"
  ],
  "author": "Community Intelligence Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/community-intelligence-platform.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/community-intelligence-platform/issues"
  },
  "homepage": "https://github.com/your-org/community-intelligence-platform#readme"
}
